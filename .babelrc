{
  "presets": [
    "@babel/preset-react",
    // preset used to let you use latest JS without managing syntax changes for your environment
    // also needed by your target environment
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "emotion"
  ]
}
